@model SearchViewModel
@{
    Layout = "~/Views/Shared/_Layout.cshtml"; }

<br />
<h2 class="text-info"> ورود اقلام فقط با فایل اکسل xlsx</h2>
<br />

<form method="post" class="col-12" enctype="multipart/form-data">
    <div class="border backgroundWhite">
        <div class="form-group row">
            <div class="col-2">
                <label asp-for="Barcode" class="col-form-label"></label>
            </div>
            <div class="col-5">
                <input asp-for="Barcode" class="form-control" />
            </div>
        </div>

        <div class="form-group row">
            <div class="col-2">
                <label asp-for="PartNo" class="col-form-label"></label>
            </div>
            <div class="col-5">
                <input asp-for="PartNo" class="form-control" />
            </div>
        </div>
        <div class="form-group row">
            <div class="col-2">
                <label class="col-form-label">فایل اکسل</label>
            </div>
            <div class="col-5">
                <input type="file" id="files" name="files" multiple accept="application/vnd.openxmlformats-officedocument.spreadsheetml.sheet" />

            </div>
        </div>


        <div class="form-group row">
            <div class="col-5 offset-2">
                <div class="row">
                    <div class="col-6">
                        <input type="submit" value="import file" class="btn btn-info form-control" />
                    </div>
                </div>

            </div>
        </div>
    </div>
</form>
